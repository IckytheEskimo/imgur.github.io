    <script>
window.addEventListener('load', function() {
    var $frontPageBeta = $('#front-page-beta');
    var betaOptInPercent = 5; // 5% of all users will get the opt in option.

    if ($.cookie('frontpagebeta') === '0') {
        return;
    }

    var dieRoll;
    if ($.cookie('fpb-roll') && parseFloat($.cookie('fpb-roll')) !== NaN) {
        dieRoll = parseFloat($.cookie('fpb-roll'));
    } else {
        dieRoll = Math.random() * 100;
        $.cookie('fpb-roll', dieRoll, {
            domain: '.imgur.com',
            path: '/',
            expires: 7
        });
    }

    if (!Imgur.Environment.auth.isLoggedIn && dieRoll > betaOptInPercent) {
        return;
    }

    // Don't show on any page other than home page.
    if (window.location.pathname !== '/'){
        return;
    }

    Imgur.Util.jafoLog({
        event: 'homepageBetaOptInAppear'
    });
    $frontPageBeta.show();
});

function frontPageBetaSignup() {
    Imgur.Util.jafoLog({
        event: 'homepageBetaOptInConfirm',
        meta: {
            method: 'voteySignMeUpDocButton',
        },
    });
    window.location = '/beta/enable';
    return false;
}

function frontPageBetaOptOut(e) {
    Imgur.Util.jafoLog({
        event: 'homepageBetaOptInReject',
        meta: {
            method: 'maybeLaterClick',
        },
    });
    $('#front-page-beta').hide();
    // Set frontpagebeta for 30 days.
    $.cookie('frontpagebeta', '0', {
        domain: '.imgur.com',
        path: '/',
        expires: 30
    });
    return false;
}
    </script>
	<div id="front-page-beta" style="display:none;" class="BetaSignUp">
		<div class="BetaSignUp-label">
			Want to try Imgur Homepage beta?
		</div>
		<div class="BetaSignUp-button">
			<a href="#" onclick="return frontPageBetaSignup()">Sign me up!</a>
		</div>
		<div class="BetaSignUp-link">
			<a href="#" onclick="return frontPageBetaOptOut()">Maybe later</a>
		</div>
	</div>
    

            
    

    

